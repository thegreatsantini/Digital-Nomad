<div class="row">
    <form method="POST" action="/profile/addressbook" class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <input name='name' id="name" type="text" class="validate">
          <label for="name">Name</label>
        </div>
        <div class="input-field col s6">
          <input name="street" id="street" type="text" class="validate">
          <label for="street">Street</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input name="city" id="city" type="text" class="validate">
          <label for="city">City</label>
        </div>
      </div>
      <div class="row">
          <div class="col s6">
            <div class="row">
              <div class="input-field col s12">
                <input name='state' type="text" id="autocomplete-input" class="autocomplete">
                <label for="autocomplete-input">State</label>
              </div>
            </div>
          </div>
        </div>
      <div class="row">
        <div class="input-field col s12">
          <input name="zipcode" id="zipcode" type="text" class="validate">
          <label for="zipcode">zipcode</label>
        </div>
      </div>
      <input type="submit" value="Save Address" class="btn">
    </form>
  </div>
<div>
  <% currentUser.savedContacts.forEach( function(item)  { %>
    <h5>Name: <%=item.name%> </h5>
    <p>Address: <%=item.street %></p>
    <p>City: <%= item.city %></p>
    <p>State: <%= item.state %></p>
    <p>Zipcode: <%= item.zipcode %> </p>
    <hr />
  <% }); %>
</div>